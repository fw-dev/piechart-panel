{
  "name": "filewave-piechart-panel",
  "version": "1.0.0",
  "description": "Grafana RSS Panel",
  "scripts": {
    "move-dist": "mv dist/ ../dist/",
    "build": "grafana-toolkit plugin:build && yarn move-dist",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev && yarn move-dist",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "author": "FileWave International",
  "license": "Apache-2.0",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@grafana/data": "6.5.2",
    "@grafana/toolkit": "6.5.2",
    "@grafana/ui": "6.5.2",
    "@types/grafana": "github:CorpGlory/types-grafana",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "jest-canvas-mock": "^2.2.0"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
